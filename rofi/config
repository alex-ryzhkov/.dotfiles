rofi.terminal:                       xfce4-terminal
rofi.kb-row-up:                      Up,Control+k,Control+p
rofi.kb-row-down:                    Down,Control+j,Control+n
rofi.kb-accept-entry:                Control+m,Return,KP_Enter
rofi.kb-remove-to-eol:               Control+Shift+e
rofi.kb-mode-next:                   Control+l,Shift+Right
rofi.kb-mode-previous:               Control+h,Shift+Left
rofi.kb-remove-char-back: BackSpace
rofi.width: 25
